{
  "version": "2",
  "plugins": [
    {
      "name": "fsharp",
      "wasm": {
        "url": "https://github.com/kaashyapan/sqlc-gen-fsharp/releases/download/latest/sqlc-gen-fsharp_1.0.0.wasm",
        "sha256": "7ab95e11314a25067f908a5e140b58fc9e40adbc2f211f8cfe9840831e87fb34"
      }
    }
  ],
  "sql": [
    {
      "schema": "postgresql/schema.sql",
      "queries": "postgresql/query.sql",
      "engine": "postgresql",
      "codegen": [
        {
          "out": "postgresql",
          "plugin": "fsharp",
          "options": {
            "namespace": "Authors",
            "async": false,
            "type_affinity": true
          }
        }
      ]
    },
    {
      "schema": "mysql/schema.sql",
      "queries": "mysql/query.sql",
      "engine": "mysql",
      "codegen": [
        {
          "out": "mysql",
          "plugin": "fsharp",
          "options": {
            "namespace": "Authors",
            "async": false,
            "type_affinity": true
          }
        }
      ]
    },
    {
      "schema": "sqlite/schema.sql",
      "queries": "sqlite/query.sql",
      "engine": "sqlite",
      "codegen": [
        {
          "out": "sqlite",
          "plugin": "fsharp",
          "options": {
            "namespace": "Authors",
            "async": false,
            "type_affinity": true
          }
        }
      ]
    }
  ]
}